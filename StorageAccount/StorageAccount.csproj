<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/JDSRAO/Azure.Solutions/tree/master/StorageAccount</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JDSRAO/Azure.Solutions</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/JDSRAO/Azure.Solutions/blob/master/LICENSE</PackageLicenseUrl>
    <PackageId>KJDS.Azure.StorageAccount</PackageId>
    <Authors>KJDS Srinivasa Rao</Authors>
    <Product>KJDS.Azure.StorageAccount</Product>
    <RepositoryType>github</RepositoryType>
    <Description>Azure storage account helpers to ease the development</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="WindowsAzure.Storage" Version="9.3.2" />
  </ItemGroup>

</Project>
